{
  "theme": {
    "name": "TabTheme"
  },
  "portalUrl": "http://www.arcgis.com",
  "appId":"senJpIJA845L4FlP",
  "title": "ArcGIS Web Application",
  "subtitle": "with ArcGIS WebApp Builder",

  "links":[
    {
      "label": "ArcGIS Online"
    }
  ],

  "preloadWidgets": {
    "widgets": [{
      "uri": "widgets/Header/Widget",
      "positionRelativeTo": "browser",
      "position": {
        "left": 0,
        "top": 0,
        "right": 0,
        "height": 44
      }
    },{
      "uri": "widgets/Footer/Widget",
      "positionRelativeTo": "browser",
      "position": {
        "left": 0,
        "right": 0,
        "bottom": 0,
        "height": 24
      }
    },{
      "uri": "widgets/SidebarController/Widget",
      "positionRelativeTo": "browser",
      "position": {
        "left": 0,
        "top": 44,
        "bottom": 24
      },
      "defaultState": "minimized",
      "config": {
        "groups": ["Measurement"],
        "widgets": "all"
      }
    }]
  },

  "map": {
    "3D": true,

    "position": {
      "left": 55,
      "top": 44,
      "right": 0,
      "bottom": 24
    },

    "basemaps": [{
      "label": "base map1",
      "url": "http://services.arcgisonline.com/ArcGIS/rest/services/ESRI_Imagery_World_2D/MapServer",
      "type": "tiled",
      "visible": true,
      "alpha": "1"
    },
    {
      "label": "elevation layer",
      "url": "http://services.arcgisonline.com/cache_w3/GlobeCache/USGS_Elevation_US_20070531",
      "type": "elevation",
      "visible": true,
      "alpha": "1"
    }],
    "operationallayers": [{
      "label": "Earthquakes",
      "url": "http://sampleserver3.arcgisonline.com/ArcGIS/rest/services/Earthquakes/EarthquakesFromLastSevenDays/FeatureServer/0",
      "type": "feature",
      "visible": true,
      "alpha": "1",
      "altitudeMode": "none"
    },{
      "label": "Beijing",
      "url": "http://10.112.18.28/ArcGIS/rest/services/ForbiddenCity/3DFeatureServer",
      "type": "3dmodle",
      "visible": true,
      "alpha": "1"
    },
    {
      "label": "San Francisco",
      "url": "http://10.112.18.28/ArcGIS/rest/services/SanFrancisco/3DFeatureServer",
      "type": "3dmodle",
      "visible": true,
      "alpha": "1"
    }],

    "mapOptions": {
      "center": [-98, 40],
      "scale": 16937199
    }
  },
  "widgetPool": {
    "panel": {
      "uri": "themes/TabTheme/panels/TabPanel/Panel"
    },

    "groups": [{
      "label": "g1",
      "widgets": [{
        "label": "Demo",
        "uri": "widgets/samplewidgets/Demo/Widget"
      }, {
        "label": "Simple",
        "uri": "widgets/samplewidgets/Simple/Widget"
      }]
    },{
      "label": "g2",
      "widgets": [{
        "label": "LayerSwipe",
        "uri": "widgets/LayerSwipe/Widget"
      }]
    }],

    "widgets": [{
      "label": "Bookmark",
      "uri": "widgets/Bookmark/Widget"
    }, {
      "label": "MapCompare",
      "uri": "widgets/MapCompare/Widget"
    }, {
      "label": "Track",
      "uri": "widgets/Track/Widget"
    }]
  }
}